{
  "name": "@kwolfsanta/amtd",
  "description": "React component library. Made to learn develope my own component library.",
  "author": "evankwolf",
  "private": false,
  "version": "0.1.1",
  "type": "module",
  "packageManager": "pnpm@7.8.0",
  "main": "dist/es/main.js",
  "module": "dist/es/main.js",
  "types": "dist/es/main.d.ts",
  "license": "MIT",
  "keywords": [
    "React",
    "Component",
    "Library"
  ],
  "homepage": "https://github.com/evankwolf/amtd",
  "repository": {
    "type": "git",
    "url": "https://github.com/evankwolf/amtd"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "sass ./src/styles/index.scss ./build/index.css",
    "lint": "eslint --ext js,ts,tsx src",
    "preinstall": "npx only-allow pnpm",
    "preview": "vite preview",
    "test": "vitest",
    "test-nowatch": "cross-env CI=true vitest",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "predeploy": "pnpm run build-storybook",
    "storybook:publish": "pnpm storybook:build && cd . >storybook-static/.nojekyll && gh-pages -d storybook-static -t true",
    "prepublish": "pnpm run test-nowatch && pnpm run lint && pnpm run build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
